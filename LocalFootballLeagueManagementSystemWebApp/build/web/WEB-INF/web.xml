<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddTeamServlet</servlet-name>
        <servlet-class>za.ac.tut.web.AddTeamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListTeamsServlet</servlet-name>
        <servlet-class>za.ac.tut.web.ListTeamsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateTeamServlet</servlet-name>
        <servlet-class>za.ac.tut.web.UpdateTeamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteTeamServlet</servlet-name>
        <servlet-class>za.ac.tut.web.DeleteTeamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPlayerServlet</servlet-name>
        <servlet-class>za.ac.tut.web.AddPlayerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListPlayersServlet</servlet-name>
        <servlet-class>za.ac.tut.web.ListPlayersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdatePlayerServlet</servlet-name>
        <servlet-class>za.ac.tut.web.UpdatePlayerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletePlayerServlet</servlet-name>
        <servlet-class>za.ac.tut.web.DeletePlayerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchPlayerServlet</servlet-name>
        <servlet-class>za.ac.tut.web.SearchPlayerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddTeamServlet</servlet-name>
        <url-pattern>/AddTeamServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListTeamsServlet</servlet-name>
        <url-pattern>/ListTeamsServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateTeamServlet</servlet-name>
        <url-pattern>/UpdateTeamServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteTeamServlet</servlet-name>
        <url-pattern>/DeleteTeamServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPlayerServlet</servlet-name>
        <url-pattern>/AddPlayerServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListPlayersServlet</servlet-name>
        <url-pattern>/ListPlayersServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdatePlayerServlet</servlet-name>
        <url-pattern>/UpdatePlayerServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletePlayerServlet</servlet-name>
        <url-pattern>/DeletePlayerServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchPlayerServlet</servlet-name>
        <url-pattern>/SearchPlayerServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
